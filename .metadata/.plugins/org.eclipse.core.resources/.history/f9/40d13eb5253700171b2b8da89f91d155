
public class Snek
{
	private static final long serialVersionUID = 1L;
	
	final int SNEKLENGTH = 1;
	private int snekHeadX;
	private int snekHeadY;
	
	public Snek(int x, int y)
	{
		snekHeadX = x;
		snekHeadY = y;
	}
	
	public int getHeadY()
	{
		return snekHeadY;
	}
	
	public int getHeadX()
	{
		return snekHeadX;
	}
	
	public void moveUp()
	{
		this.snekHeadY ++;
	}
	
	public void moveDown()
	{
		this.snekHeadY --;
	}
	
	public void moveLeft()
	{
		this.snekHeadX --;
	}
	
	public void moveRight()
	{
		this.snekHeadX ++;
	}
}
